{
  "mcpServers": {
    "dot-ai": {
      "type": "http",
      "url": "https://dot-ai.devopstoolkit.ai",
      "headers": {
        "Authorization": "Bearer ${DOT_AI_AUTH_TOKEN}"
      }
    },
    "port-vscode-eu": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.port.io/v1",
        "--header",
        "x-read-only-mode: 0"
      ]
    }
  }
}
