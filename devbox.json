{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.16.0/.schema/devbox.schema.json",
  "packages": [
    "nushell@0.110.0",
    "kubernetes-helm@3.19.1",
    "git@2.52.0",
    "vals@0.43.3",
    "yq-go@4.52.2",
    "kubectl-tree@0.4.6",
    "linode-cli@5.56.2"
  ],
  "shell": {
    "init_hook": [
      "export PATH=\"$HOME/.local/bin:$PATH\"",
      "[ -n \"$USE_VALS\" ] && eval \"$(vals env -export -f .env.vals.yaml)\"",
      "[ -f .env ] && source .env"
    ],
    "scripts": {}
  }
}
